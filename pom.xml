<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.sbt.app</groupId>
    <artifactId>token-ring</artifactId>
    <version>1</version>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>8</source>
                    <target>8</target>
